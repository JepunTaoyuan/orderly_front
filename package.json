{
  "name": "orderly-js-sdk-remix-template",
  "description": "Template for Orderly SDK v2 based on Remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run copyLocales && remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "prettier --write \"app/**/*.{ts,tsx}\"",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "copyLocales": "node scripts/copyLocales.mjs",
    "prepare": "husky"
  },
  "dependencies": {
    "@orderly.network/affiliate": "2.7.4",
    "@orderly.network/i18n": "2.7.4",
    "@orderly.network/markets": "2.7.4",
    "@orderly.network/portfolio": "2.7.4",
    "@orderly.network/react-app": "2.7.4",
    "@orderly.network/trading": "2.7.4",
    "@orderly.network/trading-leaderboard": "2.7.4",
    "@orderly.network/trading-rewards": "2.7.4",
    "@orderly.network/types": "2.7.4",
    "@orderly.network/ui": "2.7.4",
    "@orderly.network/ui-scaffold": "2.7.4",
    "@orderly.network/wallet-connector": "2.7.4",
    "@remix-run/node": "^2.15.0",
    "@remix-run/react": "^2.15.0",
    "@remix-run/serve": "^2.15.0",
    "cookie": "^1.0.2",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-plugin-cjs-interop": "^2.1.6",
    "vite-plugin-node-polyfills": "0.17.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  }
}